{
  "diagnostic.infoSign": "•",
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "graphql",
    "html",
    "yaml",
    "json",
    "python",
    "vue",
    "elixir"
  ],
  "coc.preferences.colorSupport": true,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "snippets.ultisnips.directories": ["UltiSnips", "~/.config/nvim/utils/snips"],
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": false,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  },
  "explorer.file.column.indent.indentLine": false,
  "explorer.file.column.indent.chars": " ",
  "explorer.icon.enableVimDevicons": false,
  "css.validate": false,
  "css.colorDecorators.enable": true,
  "stylelint.enable": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "vue",
    "js",
    "jsx",
    "tsx"
  ],
  "html.autoClosingTags": true,
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.defaultFormatter.js": "prettier",
  "vetur.format.defaultFormatter.postcss": "prettier",
  "vetur.format.defaultFormatter.css": "prettier",
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.bracketSpacing": true,
  "prettier.jsxSingleQuote": false,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.paths": true,
  "typescript.referencesCodeLens.enable": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.preferences.importModuleSpecifier": "auto",
  "typescript.workspaceSymbols.scope": "currentProject",
  "typescript.updateImportsOnFileMove.enable": true,
  "typescript.preferences.importModuleSpecifierEnding": "auto",
  "typescript.showUnused": true
}
